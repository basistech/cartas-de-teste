<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<servico xmlns="http://servicos.gov.br/v3/schema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://servicos.gov.br/v3/schema ../servico.xsd">
    <nome>CAU - Emissão de Certidão de Acervo Técnico</nome>
    <sigla>CAT</sigla>
    <nomes-populares/>
    <descricao><![CDATA[Emissão do documento que certifica, para os efeitos legais, o conjunto de RRTs (Registro de Responsabilidade Técnica) que compõem o acervo técnico do profissional.]]></descricao>
    <contato><![CDATA[De segunda a sexta de 9h às 19h pelo telefone 0800 883 0113 e pelo 4007 2613.]]></contato>
    <gratuito>true</gratuito>
    <solicitantes>
        <solicitante>
            <tipo>Arquitetos e Urbanistas</tipo>
            <requisitos><![CDATA[Ser registrado no CAU]]></requisitos>
        </solicitante>
    </solicitantes>
    <tempo-total-estimado>
        <ate max="30" unidade="minutos"/>
        <descricao><![CDATA[Serviço online de emissão automática pelo sistema.]]></descricao>
    </tempo-total-estimado>
    <etapas>
        <etapa>
            <titulo>Emissão</titulo>
            <descricao><![CDATA[Acessar o SICCAU com CPF e senha para emitir a CAT.]]></descricao>
            <documentos>
                <default>
                    <item>Número dos documentos de RRT que deseja incluir na CAT. Os RRTs deverão estar com baixa.</item>
                </default>
            </documentos>
            <custos>
                <default/>
            </custos>
            <canais-de-prestacao>
                <default>
                    <canal-de-prestacao tipo="web-emitir">
                        <descricao><![CDATA[https://servicos.caubr.gov.br/]]></descricao>
                    </canal-de-prestacao>
                </default>
            </canais-de-prestacao>
        </etapa>
    </etapas>
    <orgao id=""/>
    <segmentos-da-sociedade>
        <item>Cidadãos</item>
        <item>Órgãos e entidades públicas</item>
        <item>Empresas</item>
    </segmentos-da-sociedade>
    <areas-de-interesse/>
    <palavras-chave>
        <item>CAT</item>
        <item>certidão de acervo</item>
        <item>acervo técnico</item>
    </palavras-chave>
    <legislacoes>
        <item><![CDATA[Resolução Nº 93/2014]]></item>
    </legislacoes>
</servico>
