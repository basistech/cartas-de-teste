<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<servico xmlns="http://servicos.gov.br/v3/schema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://servicos.gov.br/v3/schema ../servico.xsd">
    <nome><![CDATA[CAU - Tabela de Honorários]]></nome>
    <sigla/>
    <nomes-populares/>
    <descricao><![CDATA[Aplicativo para cálculo de honorários a serem cobrados para prestação de serviços de arquitetura e urbanismo, conforme tabela indicativa de honorários definida pelo CAU.]]></descricao>
    <contato><![CDATA[De segunda a sexta de 9h às 19h pelo telefone 0800 883 0113 e pelo 4007 2613.]]></contato>
    <gratuito>true</gratuito>
    <solicitantes>
        <solicitante>
            <tipo>Arquitetos e Urbanistas</tipo>
            <requisitos><![CDATA[Com registro no CAU que possuem senha do SICCAU.]]></requisitos>
        </solicitante>
    </solicitantes>
    <tempo-total-estimado>
        <entre max="40" min="10" unidade="minutos"/>
        <descricao><![CDATA[Depende muito da complexidade do serviço a ser calculado.]]></descricao>
    </tempo-total-estimado>
    <etapas>
        <etapa>
            <titulo>Login</titulo>
            <descricao><![CDATA[Acessar o site da Tabela de Honorários e realizar o login para fazer os cálculos. Caso não seja arquiteto, será necessário realizar o cadastro antes, informando nome, e-mail e senha.]]></descricao>
            <documentos>
                <default/>
            </documentos>
            <custos>
                <default/>
            </custos>
            <canais-de-prestacao>
                <default>
                    <canal-de-prestacao tipo="web">
                        <descricao><![CDATA[http://honorario.caubr.gov.br/]]></descricao>
                    </canal-de-prestacao>
                </default>
            </canais-de-prestacao>
        </etapa>
    </etapas>
    <orgao id=""/>
    <segmentos-da-sociedade>
        <item>Cidadãos</item>
        <item>Órgãos e entidades públicas</item>
        <item>Empresas</item>
    </segmentos-da-sociedade>
    <areas-de-interesse/>
    <palavras-chave>
        <item>tabela</item>
        <item>honorarios</item>
        <item>cau</item>
        <item>valor de projeto</item>
        <item>quanto cobra um arquiteto</item>
    </palavras-chave>
    <legislacoes>
        <item><![CDATA[Resoluções Nº 64/2013 e 76/2014]]></item>
        <item><![CDATA[Lei 12.378/2010]]></item>
    </legislacoes>
</servico>
